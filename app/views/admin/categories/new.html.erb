<h1>Create a Category</h1>

<%= form_for @category, url: admin_categories_path(@category) do |form| %>

<div class="field">
  <%= form.label :title %>
  <%= form.text_field :title %>
</div>

<div class="actions">
  <%= form.submit "Create Category" %>
</div>
<% end %>


<%= link_to 'Back to Categories Index', admin_categories_path(current_user) %>
